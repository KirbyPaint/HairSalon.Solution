@{
  Layout = "_Layout";
}

<h1>Client Details: </h1>

<h3>@Model["client"].Title</h3>
<p>@Model["client"].Description</p>
<p>@Model["client"].Price</p>
<p>@Model["client"].Date</p>
<h5>Stylist: @Model["stylist"].Name</h5>

<p><a href='/stylists/@Model["stylist"].Id/clients/new'>Add another client to this stylist</a></p>
<p><a href='/stylists'>View all stylists</a></p>