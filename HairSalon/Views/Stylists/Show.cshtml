@{
  Layout = "_Layout";
}

@using HairSalon.Models;

<h3>Here are all this stylist's clients (Click on the client for full client details):</h3>
 
<ol>
@foreach (Client client in @Model["clients"])
{
  <li><a href='/stylists/@Model["stylist"].Id/clients/@client.Id'>@client.Title</a></li>
}
</ol>

<p><a href='/stylists/@Model["stylist"].Id/clients/new'>Add a new client</a></p>
<p><a href='/stylists'>Return to stylist list</a></p>